{
    "name": "asterisksyntaxhighlighter-sufficit",
    "displayName": "Asterisk Syntax Highlighter (Sufficit)",
    "description": "Syntax highlighting for Asterisk dialplan",
    "version": "1.1.4",
    "publisher": "sufficit",
    "engines": {
        "vscode": "^1.13.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "contributes": {
        "languages": [
            {
                "id": "asterisk",
                "aliases": [
                    "Asterisk",
                    "asterisk"
                ],
                "extensions": [
                    ".conf"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "asterisk",
                "scopeName": "source.asterisk",
                "path": "./syntaxes/asterisk.tmLanguage"
            }
        ],
        "snippets": [
            {
                "language": "asterisk",
                "path": "./snippets/snippets.json"
            }
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/sufficit/vscode-asterisk.git"
    },
    "dependencies": {
        "asterisksyntaxhighlighter-sufficit": "file:"
    }
}
